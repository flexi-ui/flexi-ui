{
  "name": "flexi-ui",
  "version": "1.0.0",
  "license": "MIT",
  "description": "",
  "main": "index.js",
  "private": "true",
  "keywords": [],
  "author": {
    "name": "Muneeb Mughal",
    "email": "muneeb.it.dev@gmail.com",
    "url": "https://github.com/Muneeb-Mughal-Dev"
  },
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js",
    "clean:jest": "jest --clearCache",
    "changeset:add": "npx changeset add",
    "changeset:version": "npx changeset version",
    "changeset:release": "npx changeset publish",
    "changeset:beta": "changeset pre enter beta",
    "changeset:empty": "npx changeset add --empty",
    "dev": "npx nx run-many --target=dev --all",
    "clean": "npx nx run-many --target=clean --all && rm -rf node_modules .nx pnpm-lock.yaml",
    "build": "npx nx run-many --target=build --all",
    "dev:pnpm": "pnpm --parallel --stream -r run dev",
    "build:pnpm": "pnpm --parallel --stream -r run build",
    "test": "jest --verbose --config ./jest.config.js"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.11",
    "@changesets/get-github-info": "^0.6.0",
    "@changesets/get-release-plan": "^4.0.6",
    "@changesets/types": "^6.0.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.17.0",
    "@swc/core": "^1.10.4",
    "@swc/jest": "^0.2.37",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "catalog:",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^16.2.6",
    "nx": "^22.0.2",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "typescript-eslint": "^8.19.0",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "@changesets/get-github-info": "0.6.0"
    }
  },
  "engines": {
    "node": ">=22.x",
    "pnpm": ">=9.x"
  },
  "packageManager": "pnpm@9.6.0"
}
