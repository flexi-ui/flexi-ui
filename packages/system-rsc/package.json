{
  "name": "@flexi-ui/system-rsc",
  "version": "1.0.1",
  "description": "FlexiUI system primitives compatibles with RSC imports",
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Muneeb Mughal",
    "email": "muneebmughal3324@gmail.com",
    "url": "https://github.com/Muneeb-Mughal-Dev"
  },
  "homepage": "https://github.com/awalsolution",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexi-ui/flexi-ui.git"
  },
  "bugs": {
    "url": "https://github.com/flexi-ui/flexi-ui/issues"
  },
  "keywords": [
    "system-rsc"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "node": "./dist/lib.umd.cjs",
      "types": "./dist/main.d.ts",
      "import": "./dist/lib.js",
      "require": "./dist/lib.umd.cjs",
      "default": "./dist/lib.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev:watch": "pnpm build --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@flexi-ui/theme": ">=1.0.0",
    "react": ">=18 || >=19.0.0-rc.0"
  },
  "devDependencies": {
    "@flexi-ui/react-utils": "workspace:*",
    "@flexi-ui/shared-utils": "workspace:*",
    "@flexi-ui/theme": "workspace:*",
    "@types/prop-types": "^15.7.14",
    "@types/react": "catalog:",
    "prop-types": "^15.8.1",
    "react": "catalog:",
    "tailwind-variants": "^0.3.0",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:"
  },
  "dependencies": {
    "@react-types/shared": "3.26.0",
    "clsx": "^2.1.1"
  }
}
