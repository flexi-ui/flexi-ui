{
  "name": "@flexi-ui/react-utils",
  "version": "1.0.0",
  "description": "A set of utilities for react on client side",
  "type": "module",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Muneeb Mughal",
    "email": "muneebmughal3324@gmail.com",
    "url": "https://github.com/Muneeb-Mughal-Dev"
  },
  "homepage": "https://github.com/awalsolution",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexi-ui/flexi-ui.git"
  },
  "bugs": {
    "url": "https://github.com/flexi-ui/flexi-ui/issues"
  },
  "keywords": [
    "react-utils"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "node": "./dist/lib.umd.cjs",
      "types": "./dist/main.d.ts",
      "import": "./dist/lib.js",
      "require": "./dist/lib.umd.cjs",
      "default": "./dist/lib.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev:watch": "pnpm build --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18 || >=19.0.0-rc.0"
  },
  "dependencies": {
    "@flexi-ui/shared-utils": "workspace:*",
    "@flexi-ui/react-rsc-utils": "workspace:*"
  },
  "devDependencies": {
    "@react-types/shared": "catalog:",
    "@types/react": "catalog:",
    "vite": "catalog:",
    "react": "catalog:",
    "vite-plugin-dts": "catalog:"
  }
}
