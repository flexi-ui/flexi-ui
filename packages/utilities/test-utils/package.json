{
  "name": "@flexi-ui/test-utils",
  "version": "1.0.0",
  "description": "A set of utilities for react testing",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Muneeb Mughal",
    "email": "muneeb.it.dev@gmail.com",
    "url": "https://github.com/Muneeb-Mughal-Dev"
  },
  "homepage": "https://github.com/awalsolution",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexi-ui/flexi-ui.git"
  },
  "bugs": {
    "url": "https://github.com/flexi-ui/flexi-ui/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "test-utils"
  ],
  "files": [
    "dist/*"
  ],
  "main": "./src/main.ts",
  "scripts": {
    "build": "tsc && vite build",
    "dev:watch": "pnpm build --watch",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf node_modules dist"
  },
  "clean-package": "../../../clean-package.config.json",
  "dependencies": {
    "@testing-library/user-event": "^14.5.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "vite": "catalog:",
    "clean-package": "2.2.0",
    "vite-plugin-dts": "catalog:"
  }
}
